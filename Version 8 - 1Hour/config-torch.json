        "loglevel": "DEBUG",
        "logfile": "/freqtrade/user_data/logs/freqtrade.log"
    },
    "logger": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "level": "INFO",
        "file": "/path/to/logfile.log",
        "tensorboard": true
